   <property name="styleSheet">
    <string notr="true">QSlider::groove:horizontal {
    border: none;
    height: 6px;
    background: #3e3e3e;      /* dark gray background */
    border-radius: 3px;
}

QSlider::sub-page:horizontal {
    background: #1DB954;       /* Spotify green for progress */
    border-radius: 3px;
}

QSlider::add-page:horizontal {
    background: #5D5D5D;       /* unfilled portion */
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: white;         /* small white handle */
    border: 2px solid #1DB954;
    width: 12px;
    height: 12px;
    margin: -4px 0;            /* center handle vertically */
    border-radius: 6px;
}

QSlider::handle:horizontal:hover {
    background: #1DB954;       /* glow effect when hovered */
    border: 2px solid white;
}

QSlider::handle:horizontal:pressed {
    background: #1ed760;       /* lighter Spotify green */
    border: 2px solid #1DB954;
}</string>
